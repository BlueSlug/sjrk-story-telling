<div class="sjrk-st-page-header">
    <span class="sjrk-st-page-header-main-title">{{localizedMessages.mastheadTitle}}</span>
</div>
{{#if dynamicValues.savingEnabled}}
    <div class="sjrk-st-context-link-container">
        {{#if dynamicValues.browseLinkUrl}}
            <a href="{{dynamicValues.browseLinkUrl}}" class="sjrk-st-context-link">{{localizedMessages.browseLink_text}}</a>
        {{/if}}
        {{#if dynamicValues.buildLinkUrl}}
            <a hidden href="{{dynamicValues.buildLinkUrl}}" class="sjrk-st-context-link">{{localizedMessages.buildLink_text}}</a>
        {{/if}}
    </div>
{{else}}
    {{#if dynamicValues.browseLinkUrl}}
        <div class="sjrk-st-context-link-container">
            <a href="{{dynamicValues.browseLinkUrl}}" class="sjrk-st-context-link">{{localizedMessages.browseLink_text}}</a>
        </div>
    {{/if}}
{{/if}}
